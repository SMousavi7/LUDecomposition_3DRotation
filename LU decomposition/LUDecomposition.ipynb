matrix = np.zeros((n, n))
constants = np.zeros((n))

for i in range(n):
    
    matrix[i] = list(map(int, input("enter coefficients: ").split()))
    constants[i] = int(input("enter constant: "))

L, U, flag = tajzie_LU(matrix, len(matrix))

